<p id="notice"><%= notice %></p>

<h1>Listing Fast Foods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fast_foods.each do |fast_food| %>
      <tr>
        <td><%= link_to 'Show', fast_food %></td>
        <td><%= link_to 'Edit', edit_fast_food_path(fast_food) %></td>
        <td><%= link_to 'Destroy', fast_food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fast food', new_fast_food_path %>
